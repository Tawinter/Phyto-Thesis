scale_y_log10(labels = function(x) format(x, scientific = TRUE)) +
scale_color_viridis() +
theme_bw() +
xlab('Nitrogen:Phosphorus')+
ylab(bquote('Log average abundance ' ~(cells~L^-1))) +
facet_grid(rows = vars(species.avg))
