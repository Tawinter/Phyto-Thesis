scale_y_log10() +
scale_x_continuous(breaks = 1:12,
labels = "J", "F", "M", "A", "M", "J", "J", "A", "S", "O", "N", "D") +
theme_classic() +
labs(x = "Month", y = "Log Total Abundance (Cells/l)")
